{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "team_name": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "api_url": {
      "type": "string"
    },
    "job_name": {
      "type": "string"
    },
    "pipeline_id": {
      "type": "integer"
    },
    "pipeline_name": {
      "type": "string"
    },
    "start_time": {
      "type": "integer"
    },
    "end_time": {
      "type": "integer"
    },
    "reap_time": {
      "type": "integer"
    },
    "rerun_number": {
      "type": "integer"
    },
    "rerun_of": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "created_by": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "team_name",
    "name",
    "status",
    "api_url",
    "job_name",
    "pipeline_id",
    "pipeline_name"
  ]
}
